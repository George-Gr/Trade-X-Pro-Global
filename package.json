{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env FORCE_NODE_POLYFILL_NAVIGATOR=1 node scripts/setup-node-env.js && cross-env FORCE_NODE_POLYFILL_NAVIGATOR=1 vite",
    "dev:clean": "rm -rf node_modules/.vite .vite dist && npm run dev",
    "dev:fresh": "rm -rf node_modules/.vite .vite dist && npm cache clean --force && npm install && npm run dev",
    "dev:rebuild": "npm run lint -- --fix && npm run dev:clean",
    "dev:validate": "node scripts/validate-fixes.js",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 node scripts/setup-node-env.js && cross-env NODE_OPTIONS=--max-old-space-size=4096 vite build",
    "build:analyze": "cross-env NODE_OPTIONS=--max-old-space-size=4096 node scripts/setup-node-env.js && cross-env NODE_OPTIONS=--max-old-space-size=4096 ANALYZE=true vite build",
    "build:dev": "cross-env NODE_OPTIONS=--max-old-space-size=4096 node scripts/setup-node-env.js && cross-env NODE_OPTIONS=--max-old-space-size=4096 vite build --mode development",
    "build:sentry": "cross-env NODE_OPTIONS=--max-old-space-size=4096 node scripts/setup-node-env.js && cross-env NODE_OPTIONS=--max-old-space-size=4096 vite build && sentry-cli sourcemaps upload-sourcemaps dist --url-prefix '~/' --strip-prefix './dist'",
    "build:production": "cross-env NODE_OPTIONS=--max-old-space-size=4096 node scripts/setup-node-env.js && cross-env NODE_OPTIONS=--max-old-space-size=4096 VITE_SENTRY_DSN=$SENTRY_DSN vite build",
    "lint": "cross-env NODE_OPTIONS=--max-old-space-size=8192 eslint .",
    "lint:fix": "cross-env NODE_OPTIONS=--max-old-space-size=8192 eslint . --fix",
    "lint:fast": "eslint . -c eslint.config.dev.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "sync-validators": "node ./scripts/sync-validators.js",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "type:strict": "tsc -p tsconfig.strict.json --noEmit",
    "type:check": "tsc --noEmit",
    "build:check": "npm run type:check && npm run lint && npm run test",
    "validate:design-system": "node scripts/validate-design-system.js",
    "convert:webp": "node scripts/convert-to-webp.js",
    "supabase:link": "supabase link",
    "supabase:status": "supabase status",
    "supabase:push": "supabase push",
    "supabase:pull": "supabase pull",
    "supabase:migrations": "supabase migrations list",
    "supabase:functions:deploy": "supabase functions deploy",
    "diagnose:terminal": "node ./scripts/find-vscode-pty-errors.cjs",
    "collect:pty-logs": "powershell -ExecutionPolicy Bypass -File ./scripts/collect-vscode-pty-logs.ps1",
    "network:check": "node -e \"console.log('Network check:', require('dns').lookup('registry.npmjs.org', (err, address) => console.log(err ? 'Offline' : 'Online')))\"",
    "network:config": "node scripts/network-config.js",
    "node:compat": "node scripts/node-compatibility.js",
    "copilot:config": "node scripts/copilot-config.js",
    "health:check": "node scripts/health-check.js"
